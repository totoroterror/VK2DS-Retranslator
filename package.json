{
  "name": "vk2ds-retranslator",
  "description": "Simple TypeScript project to publish posts from VK to Discord",
  "version": "1.0.0",
  "private": false,
  "author": {
    "name": "totoroterror",
    "email": "totoroterror@protonmail.com",
    "url": "https://github.com/totoroterror"
  },
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development node --nolazy -r ts-node/register src/index.ts",
    "lint": "eslint .",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@nekoteam/logger": "^1.2.0",
    "cross-env": "^7.0.3",
    "discord.js": "^14.1.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "markdown-escape": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/express": "^4.17.13",
    "@types/markdown-escape": "^1.1.0",
    "@types/node": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
